  "license": "GPL-3.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "flow": "flow",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "theme:build": "scripts/oco-json.js < src/theme/Kidsagree.oco > src/theme/kidsagree.json",
    "icons:build": "svgr --no-semi --replace-attr-value '#8B9396=currentColor' --no-title -d src/icons/components src/icons/svg",
    "optimize:svg": "find ./src -name *.svg -exec svgo --config '{ \"plugins\": [ { \"removeDesc\": {\"removeAny\": true} }, { \"removeTitle\": true }, { \"removeViewBox\": false } ] }' {} \\;",
    "deploy:site": "cd gallery && npm run build:production && echo 'ui.kidsagree.org' > dist/CNAME && cp public/favicon.svg dist && gh-pages -d dist",
    "publish": "npm publish --access public",
    "start": "cd gallery && npm start"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "styled-components": ">=4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
